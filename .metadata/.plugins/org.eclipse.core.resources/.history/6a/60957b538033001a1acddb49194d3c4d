package tree;

class ArrayToBst
{
	Node root=new Node();
	Node c=new Node();
	class Node
	{
		Node left;
		int data;
		Node right;
	}
	
     void makingRoot(int a[])
     {   	 
    	  int mid=a.length/2;
          int Root=a[mid];
          root.left=null;
          root.data=Root;
          root.right=null;        
          c=root; 
     }
     
     void UsingLoop(int a[])
     {
    	 for(int i=0;i<a.length;i++)
    	 {
    		 ConvertToNode(a,i);
    	 }
     }
     
     void ConvertToNode(int a[],int i)
     {
    	 arrayToBst(root,a[i]);
     }
     
     void arrayToBst(Node c,int data)
     {
    	 if(c==null)
    	 {
    		 return;
    	 }
    	 else 
    	 {
    		 if(c.data>)
    	 }
     }
}
public class Problem10 {

	public static void main(String[] args) {
	    ArrayToBst obj=new ArrayToBst();
	    int a[]= {1,5,9,13,15};
	    obj.makingRoot(a);
         obj.UsingLoop(a);
	}

}
