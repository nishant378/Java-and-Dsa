package linkedlist;


class SortInsert
{
	Node c=new Node();
	Node start=new Node();
	class Node
	{
		int data;
		Node next;
	}
	void firstdata(int data)
	{		
		start.data=data;
		start.next=null;
		c=start;
	}
	void enterdata(int data)
	{
		Node newnode=new Node();
		newnode.data=data;
		newnode.next=null;
		c.next=newnode;
		c=c.next;
	}
	void display()
	{
		c=start;
		while(c!=null)
		{
			System.out.println(c.data);
			c=c.next;
		}
	}
}
public class Problem3 {

	public static void main(String[] args) {
	      SortInsert obj=new SortInsert();
	  	obj.firstdata(10);	
		obj.enterdata(20);
		obj.enterdata(30);
		obj.display();	

	}

}
