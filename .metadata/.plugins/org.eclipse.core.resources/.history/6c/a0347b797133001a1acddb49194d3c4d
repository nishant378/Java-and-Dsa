package tree;

class ArrayToBst
{
	Node root=new Node();
	Node c=new Node();
	class Node
	{
		Node left;
		int data;
		Node right;
	}
     void makingRoot(int a[])
     {
    	  int mid=a.length/2;
          int root=a[mid];
          Node newnode=new Node();
          newnode.left=null;
          newnode.data=root;
          newnode.right=null;
     }
}
public class Problem10 {

	public static void main(String[] args) {
	    ArrayToBst obj=new ArrayToBst();
	    int a[]= {1,5,9,13,15};
	    obj.makingRoot(a);
      
	}

}
