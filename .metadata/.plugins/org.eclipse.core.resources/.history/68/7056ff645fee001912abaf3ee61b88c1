package recursion;
import java.math.BigInteger;
public class Power {

	public static void main(String[] args) {
	   
	 BigInteger base=new BigInteger("5");
	 int power=3;
	 BigInteger dumy=new BigInteger("1");
	 for(int i=0;i<=power;i++)
	 {
		 dumy=dumy.multiply(base);
	 }
	 System.out.println(dumy);

	}

}
