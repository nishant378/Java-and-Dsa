package queue;
import java.util.*;
public class Problem3 {

	public static void main(String[] args) {
	Stack<Integer> stack=new Stack<Integer>();
	stack.push(1);
	stack.push(2);
	stack.push(3);
	stack.pop();
          System.out.println(stack);
          System.out.println(stack.indexOf(1));
          System.out.println(stack.peek());
	}

}
