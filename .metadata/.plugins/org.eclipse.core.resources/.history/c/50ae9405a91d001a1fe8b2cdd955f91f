package linkedlist;

class LinkedList
{
	Node c=new Node();
	class Node
	{
		int data;
		Node next;
	}
	void firstdata(int data)
	{
		Node obj=new Node();
		obj.data=data;
		obj.next=null;
		c=obj;
	}
	void enterdata(int data)
	{
		Node new=new Node();
		new.data=data;
		new.next=null;
		new.next=obj;
		new.next=c;
	}
}
public class CreateLink {

	public static void main(String[] args) {
		LinkedList obj=new LinkedList();
		obj.firstdata(1);
		obj.enterdata(2);
		obj.enterdata(3);
				System.out.println(obj);

	}

}
