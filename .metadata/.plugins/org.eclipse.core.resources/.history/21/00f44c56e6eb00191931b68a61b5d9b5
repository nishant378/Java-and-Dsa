package recursion;
class C
{ 	
	static void  fib(int a,int b)
	{
	
			if( b>=500)
			{
		       return;
			}
			else
			{
				int c=a+b;
				System.out.println(c);
				a=b;
				b=c;
				fib(a,b);	
		}
	}
}
public class Fib_Recursion {

	public static void main(String[] args) {
		
		int a=0;
		int b=1;
      System.out.println(a);
	System.out.println(b);
	C.fib(a,b);
	
	}

}
