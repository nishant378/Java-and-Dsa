package array;

public class Test {
 
	public static void main(String[] args) {
	int a[][]=new int[2][2];
	int b[][]=new int[2][2];
	int c[][]=new int[2][2];
	System.out.println("Enter 1 matrix");
	for(int i=0;i<=1;i++)
	{
		for(int j=0;j<=1;j++)
		{
			System.out.println(a[i][j]);
		}
		
	}
	System.out.println("Enter 2 matrix");
	for(int i=0;i<=1;i++)
	{
		for(int j=0;j<=1;j++)
		{
			System.out.println(b[i][j]);
		}
	}
	for(int i=0;i<=1;i++)
	{
		for(int j=0;j<=1;j++)
		{
			for(int k=0;k<=1;k++)
			{
				c[i][j]=c[i][j]+(a[i][k]*a[k][j]);
			}
		}
	}
	System.out.println("resulted array");
	for(int i=0;i<=1;i++)
	{
		for(int j=0;j<=1;j++)
		{
			System.out.println(c[i][j]);
		}
	}
       
			}
	}


