package serilization;
import java.io.*;
class Demo implements Serializable
{
	int a;
	
}
public class Serialization {

	public static void main(String[] args) {
         Demo obj=new Demo();
         obj.a=10;
         try
         {
         FileOutputStream obj1=new FileOutputStream("E:/Music/Hello.txt");
          obj1.write(4);
         }
         catch(IOException e)
         {
        	 System.out.println();
         }
	}

}
